{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "essence",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "find_statement"
        },
        {
          "type": "STRING",
          "value": "such that"
        },
        {
          "type": "SYMBOL",
          "name": "constraint"
        }
      ]
    },
    "find_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "find"
        },
        {
          "type": "SYMBOL",
          "name": "variable_list"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "domain"
        }
      ]
    },
    "variable_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "variable"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "constraint": {
      "type": "SYMBOL",
      "name": "expression"
    },
    "domain": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "int"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "range"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "STRING",
          "value": ".."
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "variable": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "conjunction"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "or"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "conjunction"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "conjunction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comparison"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "and"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "comparison"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comparison": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "addition"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "comp_op"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "addition"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "addition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "term"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "+"
                      },
                      {
                        "type": "STRING",
                        "value": "-"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "term"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "factor"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "*"
                      },
                      {
                        "type": "STRING",
                        "value": "/"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "factor"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "factor": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "comp_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "!="
        },
        {
          "type": "STRING",
          "value": "<="
        },
        {
          "type": "STRING",
          "value": ">="
        },
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "constant": {
      "type": "SYMBOL",
      "name": "integer"
    },
    "integer": {
      "type": "PATTERN",
      "value": "[0-9]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
