    "single_line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "$"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "TRUE"
        },
        {
          "type": "SYMBOL",
          "name": "FALSE"
        }
      ]
    },
    "integer": {
      "type": "PATTERN",
      "value": "[0-9]+"
    },
    "TRUE": {
      "type": "STRING",
      "value": "true"
    },
    "FALSE": {
      "type": "STRING",
      "value": "false"
    },
    "variable": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
