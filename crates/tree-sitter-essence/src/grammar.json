    "domain": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_domain"
        },
        {
          "type": "SYMBOL",
          "name": "bool_domain"
        }
      ]
    },
    "int_domain": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "int"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "("
                  },
                  {
                    "type": "SYMBOL",
                    "name": "range_list"
                  },
                  {
                    "type": "STRING",
                    "value": ")"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "bool_domain": {
      "type": "STRING",
      "value": "bool"
    },
    "range_list": {
      "type": "PREC",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "lower_bound_range"
              },
              {
                "type": "SYMBOL",
                "name": "upper_bound_range"
              },
              {
                "type": "SYMBOL",
                "name": "closed_range"
              },
              {
                "type": "SYMBOL",
                "name": "integer"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "REPEAT",
                "content": {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "SYMBOL",
                          "name": "lower_bound_range"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "upper_bound_range"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "closed_range"
                        },
                        {
                          "type": "SYMBOL",
                          "name": "integer"
                        }
                      ]
                    }
                  ]
                }
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "lower_bound_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "STRING",
          "value": ".."
        }
      ]
    },
    "upper_bound_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ".."
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "closed_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "STRING",
          "value": ".."
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        }
      ]
    },
    "variable": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
