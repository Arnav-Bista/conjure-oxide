    "conjunction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comparison"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "and"
                  },
