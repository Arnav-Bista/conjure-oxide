    "program": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "find_statement_list"
          },
          {
            "type": "SYMBOL",
            "name": "constraint_list"
          }
        ]
      }
