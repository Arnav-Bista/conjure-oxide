                    "type": "SYMBOL",
                    "name": "comparison"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "comparison": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "addition"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "comp_op"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "addition"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "addition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "term"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "+"
                      },
                      {
                        "type": "STRING",
                        "value": "-"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "term"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "term": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "factor"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "*"
                      },
                      {
                        "type": "STRING",
                        "value": "/"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "factor"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
