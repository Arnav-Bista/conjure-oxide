                  {
                    "type": "SYMBOL",
                    "name": "variable"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "constraint_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "such that"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "constraint"
          }
        }
      ]
    },
    "constraint": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "not"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "not": {
