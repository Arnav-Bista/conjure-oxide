{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "essence",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "find_statement_list"
          },
          {
            "type": "SYMBOL",
            "name": "constraint_list"
          }
        ]
      }
    },
    "single_line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "$"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "find_statement_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "find"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "find_statement"
          }
        }
      ]
    },
    "find_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_list"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "domain"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "variable_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
